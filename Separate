require(stringr)
my_func <- function(code, i = 1, code_fixed = c()) {
  x <- str_split(code, "")
  while (i <= str_length(code)) {
    if (is.na(as.integer(x[[1]][i])) == TRUE) {
      code_fixed = append(code_fixed, substr(code, i, i+2))
    }
    i <- i + 1
  }
  print(code_fixed)
}
my_func("R123B123") #test
